    <script type="text/javascript">	

		jQuery(function() {

			var currentLiIndex = 0,
				liWidth = 913,
				$carouselWrapper = jQuery('ul.carousel'),
				totalLis = $carouselWrapper.children('li').length,
				$currentVideoLi = jQuery("#currentVideoLi");

			function nextCarousel() {
				if(currentLiIndex == totalLis-1) {
					$carouselWrapper.animate({
				    	left: "0"
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_right');
				  	});
				  	currentLiIndex = 0;
				} else {
					currentLiIndex++;
					$carouselWrapper.animate({
				    	left: "-" + (liWidth * currentLiIndex)
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_right');
				  	});
				}
				if($currentVideoLi.length > 0)
					$currentVideoLi.text(currentLiIndex + 1);
			}

			function prevCarousel() {
				if(currentLiIndex == 0) {
					$carouselWrapper.animate({
				    	left: "-" + ((totalLis - 1) * liWidth)
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_left');
				  	});
				  	currentLiIndex = totalLis -1;
				} else {
					currentLiIndex--;
					$carouselWrapper.animate({
				    	left: (liWidth * currentLiIndex)*(-1)
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_left');
				  	});
				}
				if($currentVideoLi.length > 0)
					$currentVideoLi.text(currentLiIndex + 1);
			}

			function videoCarouselInit(jsonObj) {
				if($currentVideoLi.length > 0)
					jQuery("#totalVideoLi").text(totalLis);

				jQuery('#videohaveyouseen div.controls a').on('click', function(e) {
					e.preventDefault();
					if(jQuery(this).hasClass('pg-nextBtn'))
						nextCarousel.call(this);
					else if(jQuery(this).hasClass('pg-prevBtn'))
						prevCarousel.call(this);
			    });

			    jQuery('#videohaveyouseen ul.carousel li a').on("click", function(e) {
			    	e.preventDefault();
			    	jsonObj[0].playlist["initialIndex"] = jQuery(this).data('index');
			    	jQuery.publish('launchVideoOverlay', jsonObj);
			    	anTrackLink(this, 'espn', 'page1videostripe', 'video_'+jQuery(this).data('videoid'));
			    });
